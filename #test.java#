class Gizmo {
    public String getMaker() {

    }
    public boolean isElectronic() {

    }
    public boolean equals(Object other) {

    }
}

class OnlinePurchaseManager {
    private ArrayList<Gizmo> purchases;
    public int countElectronicsByMaker(String maker) {
	int count = 0;
	for (Gizmo buff : purchases)
	    if (maker.compareTo(buff.getMaker()) == 0)
		count++;
	return count;
    }
    public boolean hasAdjacentEqualPair() {
	Gizmo previous = purchases.get(0);
	for (int i = 1; i < purchases.size(); i++) {
	    if (previous.equals(purchases.get(i)))
		return true;
	    previous = purchases.get(i);
	}
	return false;
    }
    public Gizmo getCheapestGizmoByMaker(String maker) {}
    /*
     *Need to include the price in the Gizmo object.
     *Can access this from the purchases ArrayList.
     *Function in Gizmo that returns the price in dollars as a double.
     */
}



class CheckDigit {
    public static int getcheck(int num) {

    }
    public static boolean isValid(int numWithCheckDigit) {
	return ((numWithCheckDigit%10) == getcheck(numWithCheckDigit));
    }
    /*
